VehicleType { 
   Name = SHELL
   parent = 363
   ID = 1260

   Description = Transport UBoot
   picture = mk4s_shell.png
   Infoimage = mk4s_shell.jpg
   wreckageobject = 14260
   GuideSortHelp += 0 0 0 0 4 0 0 0 0 0
   InfoText = [Speziell für die Unterstützung der U-Boot-Flotte auf See wurde das Shell entwickelt. Dieses U-Boot bietet die Möglichkeit, andere Schiffe zu betanken und auch begrenzt aufzumunitionieren. Als U-Boot ist es dabei für den Gegner nur schwer aufzuspüren. Ebenso verfügt es über einen großen Laderaum, womit der Transport bis zu sechs leichterer Einheiten möglich ist. So lassen sich beispielsweise auch GT-Dive aussetzen, um überraschende Angriffe durchzuführen. Das Shell kann auch selbst Torpedos abfeuern. Die Panzerung ist allerdings schwächer als bei den Kampf-U-Booten. Deshalb sollte immer ausreichender Schutz gewährleistet werden. ]
   
   Transportation ->* VehicleType.TransSubmarine 
   Tank { 
      Material = 10000
      fuel = 20000
   } Tank
   Features += ExternalFuelTransfer ExternalMaterialTransfer automatic_resource_searching 
   ResourceDrillingRange = 3

   Weapons { 
      Initiative = 0
      Number = 5
      Weapon0 ->* VehicleType.ServiceGround 

      Weapon1 ->* VehicleType.UTorp 
      Weapon1.Type = torpedo ammo_transfer shootable 
      Weapon1.Ammo = 20

      Weapon2 ->* VehicleType.MG_Tank 
      Weapon2.Type = machinegun ammo_transfer shootable 
      Weapon2.Ammo = 50

      Weapon3 ->* VehicleType.AmmoCM 
      Weapon3.Ammo = 6

      Weapon4 ->* VehicleType.AmmoBomb 
      Weapon4.Ammo = 30
   } Weapons
} VehicleType
